{
  "name": "takatost/laravel-pubsub",
  "description": "A Pub-Sub abstraction for Laravel",
  "license": "MIT",
  "authors": [
    {
      "email": "takatost@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^5.3",
    "superbalist/php-pubsub": "^1.0",
    "superbalist/php-pubsub-redis": "^1.0",
    "superbalist/php-pubsub-google-cloud": "^2.0",
    "illuminate/console": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Superbalist\\LaravelPubSub\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  }
}
