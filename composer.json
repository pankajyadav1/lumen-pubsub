{
  "name": "takatost/lumen-pubsub",
  "description": "A Pub-Sub abstraction for Lumen",
  "license": "MIT",
  "authors": [
    {
      "name": "takatost",
      "email": "takatost@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": ">=5.2",
    "superbalist/php-pubsub": "^1.0",
    "superbalist/php-pubsub-redis": "^1.0",
    "takatost/php-pubsub-cmq": "~1.0",
    "superbalist/php-pubsub-google-cloud": "^2.0",
    "illuminate/console": ">=5.2"
  },
  "autoload": {
    "psr-4": {
      "Takatost\\LumenPubSub\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  }
}
